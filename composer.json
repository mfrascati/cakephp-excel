{
    "name": "robotusers/cakephp-excel",
    "description": "Robotusers CakePHP Excel plugin",
    "homepage": "https://github.com/robotusers/cakephp-excel",
    "type": "cakephp-plugin",
    "license": "MIT",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/mfrascati/cakephp-filesystem"
        },
    ],
    "require": {
        "php": ">=8.3",
        "mfrascati/cakephp-filesystem": "dev-main",
        "phpoffice/phpspreadsheet": "^1.16"
    },
    "require-dev": {
        "cakephp/cakephp": "^5.0",
        "cakephp/cakephp-codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Robotusers\\Excel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests",
            "Robotusers\\Excel\\Test\\": "tests"
        }
    }
}
